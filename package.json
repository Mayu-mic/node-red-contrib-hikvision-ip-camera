{
  "name": "node-red-contrib-hikvision-ip-camera",
  "version": "1.0.1",
  "description": "Hikvision ip camera client for Node-RED",
  "main": "dist/nodes/face-detection.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:node": "npm run copy:html && tsc",
    "build:client": "cd client/linux64/proj && make clean && make",
    "copy:html": "mkdir -p dist/nodes/ && cpx \"nodes/*.html\" \"dist/nodes/\"",
    "clean": "npm-run-all clean:node clean:client",
    "clean:node": "rimraf ./dist",
    "clean:client": "cd client/linux64/proj && make clean",
    "postinstall": "npm run build"
  },
  "node-red": {
    "nodes": {
      "face-detection": "dist/nodes/face-detection.js",
      "ivms-config": "dist/nodes/ivms-config.js"
    }
  },
  "keywords": [],
  "author": "Ryo Koizumi <koizumiryo@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mayu-mic/node-red-contrib-hikvision-ip-camera.git"
  },
  "os": [
    "linux"
  ],
  "devDependencies": {
    "@types/node-red": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "cpx": "^1.5.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2"
  }
}
